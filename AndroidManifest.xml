<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blazblueframedata"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.blazblueframedata.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.blazblueframedata.SimpleTable"
            android:label="@string/title_character_list" >
        </activity>
                <activity
            android:name="com.example.blazblueframedata.blazblueinfo.BlazblueTable"
            android:label="@string/title_character_list" >
        </activity>
        <activity
            android:name="com.example.blazblueframedata.CharacterListActivity"
            android:label="@string/title_character_list" >
        </activity>
        <activity
            android:name="com.example.blazblueframedata.CharacterDetailActivity"
            android:label="@string/title_character_detail" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CharacterListActivity" />
        </activity>
    </application>

</manifest>
